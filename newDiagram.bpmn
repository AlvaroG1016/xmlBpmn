<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
                     xmlns:zeebe="http://camunda.org/schema/zeebe/1.0">
    <bpmn:process id="my-process">
      <bpmn:startEvent id="start">
        <bpmn:outgoing>sequenceFlow</bpmn:outgoing>
      </bpmn:startEvent>
      <bpmn:sequenceFlow id="sequenceFlow" sourceRef="start" targetRef="task" />
      <bpmn:task id="task" zeebe:taskDefinitionKey="my-task">
        <bpmn:incoming>sequenceFlow</bpmn:incoming>
      </bpmn:task>
    </bpmn:process>
  </bpmn:definitions>
